<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.iotsmartbicyclelock">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.BLUETOOTH_CONNECT" />

    <application
        android:name=".sharedPreferences.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_my_image"
        android:label="JinUkTest"
        android:roundIcon="@mipmap/ic_my_image_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.IoTSmartBicycleLock">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>